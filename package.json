{
  "name": "sc-fam",
  "packageManager": "yarn@4.8.1",
  "scripts": {
    "dev": "yarn workspace @sc-fam/website dev",
    "build": "yarn workspace @sc-fam/website build",
    "start": "yarn workspace @sc-fam/website start",
    "lint": "eslint && yarn workspaces foreach --all run build:no-emit",
    "lint:fix": "eslint --fix . && prettier --write \"**/*.{js,mjs,jsx,ts,mts,tsx,json,css,scss,md}\"",
    "test": "vitest --run"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.23.0",
    "eslint-config-next": "15.2.5",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "58.0.0",
    "jiti": "2.4.2",
    "jsdom": "26.0.0",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0",
    "vitest": "3.1.1"
  },
  "workspaces": [
    "packages/*"
  ]
}
